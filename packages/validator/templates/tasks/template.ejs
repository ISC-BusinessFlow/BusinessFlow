import { TaskRule } from '../base.ts';

export const task<%= id %> = new TaskRule({
  name: <%= name %>,
  id: <%= id %>,
  from: {
    allowTasks: <%= from.allowTasks %>
    allowPaths: <%= from.allowPaths %>
    denyTasks: <%= from.denyTasks %>
    denyPaths: <%= from.denyPaths %>
  }
  to: {
    allowTasks: <%= to.allowTasks %>
    allowPaths: <%= to.allowPaths %>
    denyTasks: <%= to.denyTasks %>
    denyPaths: <%= to.denyPaths %>
  }
})
